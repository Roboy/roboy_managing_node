<launch>
	<!-- Load the parameters to the parameter server from the yaml file-->
	<node name="roboy_realsense"
                pkg="roboy_realsense"
                type="roboy_realsense"
                respawn="false"
                output="screen"
        />
	<node name="interface"
            pkg="interface"
            type="interface"
            respawn="false"
            output="screen"
            launch-prefix="gdb -ex run"
    />
</launch>

